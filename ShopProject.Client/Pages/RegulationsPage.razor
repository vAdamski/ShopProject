@page "/Regulations"
@inject NavigationManager NavigationManager

<div class="container">
    <h3>Regulamin strony</h3>
    <div class="row">
        <div class="col-12">
            <RegulationContent/>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <button class="btn btn-primary" @onclick="Back">Back</button>
        </div>
    </div>
</div>

@code {
    [Parameter] public string BackUrl { get; set; }
    
    private void Back()
    {
        NavigationManager.NavigateTo("/cart");
    }
}